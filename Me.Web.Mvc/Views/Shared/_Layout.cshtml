@using Me.Web.Mvc.Common
@using MvcMiniProfiler
<!doctype html>
<!--[if lt IE 7]> <html class="no-js ie6 oldie" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js ie7 oldie" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js ie8 oldie" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
	<meta charset="utf-8">
	<title>@(!string.IsNullOrWhiteSpace(ViewBag.Title) ? ViewBag.Title + " | " : string.Empty)David Wick</title>
	<meta name="description" content="David Wick's professional resume.">
	<meta name="author" content="David Wick">
	<meta name="viewport" content="width=device-width,initial-scale=1">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/style.css")">
    @RenderSection("css", false)
	<script src="@Url.Content("~/Content/js/libs/modernizr-2.0.6.min.js")"></script>
</head>
<body>
    @if(HttpContext.Current.Request.IsDev() || Request.IsLocal)
    {
        <button id="grid-toggle">Show grid</button>
        <div class="container">
            <div id="grid-helper">
                <div class="container">
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                    <div class="col one"></div>
                </div>
            </div>
        </div>
    }
    <header id="header" role="banner">
        <div class="container">
            <nav role="navigation">
                <ul>
                    <li class="social linkedin"><a href="http://linkedin.com/in/dwick" target="_blank" title="David on Linkedin">Linkedin</a></li>
                    <li class="social twitter"><a href="http://twitter.com/davidwick_" target="_blank" title="David on Twitter">Twitter</a></li>
                    <li class="social facebook"><a href="https://www.facebook.com/davidwick" target="_blank" title="David on Facebook">stack<b>overflow</b></a></li>
                </ul>
            </nav>
        </div>
    </header>
    <div id="main" role="main">
        <div class="container">
            @RenderBody()
        </div>
    </div>
    <footer role="contentinfo">
    
    </footer>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="@Url.Content("~/Content/js/libs/jquery-1.6.2.min.js")"><\/script>')</script>
    <script defer src="@Url.Content("~/Content/js/script.js")"></script>
    <script>
        window._gaq = [['_setAccount', 'UA-25375575-1'], ['_trackPageview'], ['_trackPageLoadTime']];
        Modernizr.load({
            load: ('https:' == location.protocol ? '//ssl' : '//www') + '.google-analytics.com/ga.js'
        });
    </script>
    @MiniProfiler.RenderIncludes(showTrivial: true, showTimeWithChildren: true, position: RenderPosition.Right)
    @RenderSection("js", false)
    <!--[if lt IE 7 ]>
    	<script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.2/CFInstall.min.js"></script>
    	<script>window.attachEvent("onload",function(){CFInstall.check({mode:"overlay"})})</script>
    <![endif]-->
</body>
</html>
